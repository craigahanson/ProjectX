@model PersonModel
@{
    ViewData["Title"] = "Person";
}

<h1>Person</h1>

<div>
    <label for="personid" class="col-sm-2 col-form-label">Id</label>
    <div class="col-sm-10">
        <label readonly class="form-control-plaintext" id="personid">@Model.Id</label>
    </div>
</div>
<div>
    <label for="personfirstname" class="col-sm-2 col-form-label">First Name</label>
    <div class="col-sm-10">
        <label readonly class="form-control-plaintext" id="personfirstname">@Model.FirstName</label>
    </div>
</div>
<div>
    <label for="personlastname" class="col-sm-2 col-form-label">Last Name</label>
    <div class="col-sm-10">
        <label readonly class="form-control-plaintext" id="personlastname">@Model.LastName</label>
    </div>
</div>